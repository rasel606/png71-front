    60% {
        top: 9.0666666667vw;
        right: 8vw;
        width: 1.0666666667vw
    }

    70% {
        top: 6.1333333333vw;
        right: 1.0666666667vw;
        width: 9.3333333333vw
    }

    to {
        top: 6.6666666667vw;
        right: 1.6vw;
        width: 8.5333333333vw
    }
}

